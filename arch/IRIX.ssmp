CC       = cc
CPP      = /usr/lib/cpp
FC       = f90 -freeform
FC_fixed = f90 -fixedform
LD       = f90
AR       = ar -r
CPPFLAGS = -C -D__IRIX -D__FFTSG -D__FFTSGI -P
FCFLAGS  = -D__IRIX -D__FFTSG -D__FFTSGI -O2 -mp -automatic\
           -macro_expand -u -ansi
LDFLAGS  = $(FCFLAGS) -L/usr/local/lib 
LIBS     = -lcomplib.sgimath_mp

