  &DFT
    ROKS
    MULTIP 3
    &QS
      METHOD MNDO
      &SE
       
      &END
    &END QS
    &SCF
      EPS_SCF 1.0E-6
      SCF_GUESS atomic
    &END SCF
  &END DFT
  &SUBSYS
    &CELL
      ABC 10.0 10.0 10.0
      PERIODIC NONE
    &END CELL
    &COORD
      O     0.000000     0.000000    -0.600000
      O     0.000000     0.000000     0.600000
    &END COORD
    &PRINT
      &STRUCTURE_DATA
        DISTANCE 1 2
      &END STRUCTURE_DATA
      &KINDS
        BASIS_SET
        POTENTIAL
      &END KINDS
    &END PRINT
  &END SUBSYS
&END FORCE_EVAL
