&GLOBAL
  PROJECT O2-ROKS
  RUN_TYPE GEO_OPT
  PRINT_LEVEL LOW
&END GLOBAL
&MOTION
 &GEO_OPT
  OPTIMIZER BFGS
  MAX_ITER  30
  MAX_DR    0.001
  RMS_DR    0.0005
  MAX_FORCE 0.00015
  RMS_FORCE 0.0001
 &END GEO_OPT
 &PRINT
   &STRUCTURE_DATA
     DISTANCE 1 2
   &END STRUCTURE_DATA
 &END
&END MOTION
&FORCE_EVAL
