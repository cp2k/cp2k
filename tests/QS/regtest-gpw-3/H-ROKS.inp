&GLOBAL
  PROJECT H-ROKS
  PRINT_LEVEL LOW
&END GLOBAL
&FORCE_EVAL
  METHOD Quickstep
  &DFT
    ROKS
    MULTIPLICITY 2
    BASIS_SET_FILE_NAME ../../../data/BASIS_SET
    POTENTIAL_FILE_NAME ../../../data/POTENTIAL
    &MGRID
      CUTOFF 200
    &END MGRID
    &PRINT
      &AO_MATRICES
#       CORE_HAMILTONIAN
#       DENSITY
#       KOHN_SHAM_MATRIX
#       ORTHO
#       OVERLAP
      &END AO_MATRICES
      &MO
#       EIGENVALUES
#       EIGENVECTORS
#       OCCUPATION_NUMBERS
      &END MO
    &END
    &POISSON
      PERIODIC NONE
      POISSON_SOLVER MT
    &END POISSON
    &QS
      EPS_DEFAULT 1.0E-8
    &END QS
    &SCF
      CHOLESKY OFF
      SCF_GUESS atomic
    &END SCF
    &XC
      &XC_FUNCTIONAL Pade
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 4.0 4.0 4.0
      PERIODIC NONE
    &END CELL
    &COORD
      H   0.000000  0.000000  0.000000
    &END COORD
    &KIND H
      BASIS_SET DZVP-GTH-PADE
      POTENTIAL GTH-PADE-q1
    &END KIND
    &PRINT
      &KINDS
        BASIS_SET
        POTENTIAL
      &END KINDS
    &END PRINT
  &END SUBSYS
&END FORCE_EVAL
