&GLOBAL
  PRINT_LEVEL low
  PROJECT O2-UKS-GPW-relax_multip
  RUN_TYPE energy
&END GLOBAL

&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME BASIS_SET
    MULTIPLICITY 1
    POTENTIAL_FILE_NAME POTENTIAL
    RELAX_MULTIP 1.0E-5
    UKS
    &MGRID
      CUTOFF 200
    &END MGRID
    &QS
      EPS_DEFAULT 1.0E-8
    &END QS
    &SCF
      ADDED_MOS 2 1
      SCF_GUESS atomic
    &END SCF
    &XC
      &XC_FUNCTIONAL PADE
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 4.0 4.0 6.0
      PERIODIC none
    &END CELL
    &COORD
      O  0.000000  0.000000  0.608000
      O  0.000000  0.000000 -0.608000
    &END COORD
    &KIND O
      BASIS_SET DZVP-GTH-PADE
      POTENTIAL GTH-PADE-q6
    &END KIND
    &PRINT
      &KINDS
        BASIS_SET
        POTENTIAL
      &END KINDS
      &STRUCTURE_DATA
        DISTANCE 1 2
      &END STRUCTURE_DATA
    &END PRINT
  &END SUBSYS
&END FORCE_EVAL
