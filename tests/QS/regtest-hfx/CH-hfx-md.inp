&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME ../../../data/EMSL_BASIS_SETS
    POTENTIAL_FILE_NAME ../../../data/POTENTIAL
    LSD
    &MGRID
      CUTOFF 200
      REL_CUTOFF 50
    &END MGRID
    &QS
      METHOD GAPW
    &END QS
    &SCF
      SCF_GUESS ATOMIC
      MAX_SCF 5 
    &END SCF
    &XC
       &XC_FUNCTIONAL NONE
       &END XC_FUNCTIONAL
       &HF
        &SCREENING
          EPS_SCHWARZ 1.0E-10
          SCREEN_P_FORCES FALSE
        &END
        &MEMORY
          MAX_MEMORY  10
        &END
       &END
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 5.0 5.0 5.0
    &END CELL
    &COORD
     C   0.000000    0.000000    0.1581 
     H   0.000000    0.000000    -0.9487
    &END COORD
    &KIND H
      BASIS_SET 6-31Gx
      POTENTIAL ALL
    &END KIND
    &KIND C
      BASIS_SET 6-31Gx
      POTENTIAL ALL
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT CH-hfx-md
#  TRACE
  RUN_TYPE MD 
  PRINT_LEVEL MEDIUM 
&END GLOBAL
&MOTION
 &MD
   TEMPERATURE 0.1
   TIMESTEP 0.5
   STEPS    3
 &END
&END
