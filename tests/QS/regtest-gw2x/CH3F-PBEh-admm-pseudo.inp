&GLOBAL
  PROJECT CH3F-PBEh-admm-pseudo
  PRINT_LEVEL LOW
  RUN_TYPE ENERGY
&END GLOBAL
&FORCE_EVAL
  &DFT
    BASIS_SET_FILE_NAME  EMSL_BASIS_SETS
    BASIS_SET_FILE_NAME  BASIS_MOLOPT
    BASIS_SET_FILE_NAME  BASIS_ADMM
    POTENTIAL_FILE_NAME  POTENTIAL
    AUTO_BASIS RI_XAS    MEDIUM

    &AUXILIARY_DENSITY_MATRIX_METHOD
      ADMM_PURIFICATION_METHOD MO_DIAG
    &END AUXILIARY_DENSITY_MATRIX_METHOD

    &POISSON
      PERIODIC NONE
      PSOLVER MT
    &END POISSON

    &QS
      METHOD GAPW
    &END QS

    &SCF
      &OT
         PRECONDITIONER FULL_ALL
         MINIMIZER DIIS
      &END OT
    &END SCF

    &XC
      &XC_FUNCTIONAL PBE
         &PBE
            SCALE_X 0.55
         &END PBE
      &END XC_FUNCTIONAL
      &HF
         FRACTION 0.45
      &END HF
    &END XC

    &XAS_TDP
      &DONOR_STATES
         DEFINE_EXCITED BY_KIND
         KIND_LIST F
         STATE_TYPES 1s
         N_SEARCH 1
         LOCALIZE
      &END DONOR_STATES

      DIPOLE_FORM LENGTH
      GRID F 100 250

      &GW2X
         BATCH_SIZE 1
      &END GW2X
      
      &KERNEL
         &XC_FUNCTIONAL PBE
            &PBE
               SCALE_X 0.55
            &END PBE
         &END XC_FUNCTIONAL
         &EXACT_EXCHANGE
            FRACTION 0.45
         &END EXACT_EXCHANGE
      &END KERNEL
    &END XAS_TDP

  &END DFT
  &SUBSYS
    &KIND C
      BASIS_SET DZVP-MOLOPT-SR-GTH
      BASIS_SET AUX_FIT FIT3
      POTENTIAL GTH-PBE
    &END KIND
    &KIND H
      BASIS_SET DZVP-MOLOPT-SR-GTH
      BASIS_SET AUX_FIT FIT3
      POTENTIAL GTH-PBE
    &END KIND
    &KIND F
      BASIS_SET 6-31++G**
      BASIS_SET AUX_FIT 6-31G**
      POTENTIAL ALL
    &END KIND
    &CELL
      ABC 6.0 6.0 6.0
      PERIODIC NONE
    &END CELL
    &COORD
      H   0.00260325  -0.00450550   1.10443341
      H   1.04213378  -0.00450788  -0.36568798
      H  -0.51716250  -0.90476795  -0.36569384
      F  -0.64981636   1.12551553  -0.45949111
    &END COORD
    &TOPOLOGY
      &CENTER_COORDINATES
      &END CENTER_COORDINATES
    &END TOPOLOGY
  &END SUBSYS
&END FORCE_EVAL
