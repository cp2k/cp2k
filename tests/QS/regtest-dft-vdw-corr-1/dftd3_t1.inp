&GLOBAL
  PRINT_LEVEL MEDIUM
  PROJECT dftd3_t1
  RUN_TYPE ENERGY
&END GLOBAL

&FORCE_EVAL
  METHOD QS
  &DFT
    BASIS_SET_FILE_NAME GTH_BASIS_SETS
    POTENTIAL_FILE_NAME POTENTIAL
    &MGRID
      CUTOFF 100
      NGRIDS 1
    &END MGRID
    &QS
      METHOD GPW
    &END QS
    &SCF
      EPS_SCF 1.0e-0
      IGNORE_CONVERGENCE_FAILURE
      MAX_SCF 1
      SCF_GUESS ATOMIC
    &END SCF
    &XC
      &VDW_POTENTIAL
        DISPERSION_FUNCTIONAL PAIR_POTENTIAL
        &PAIR_POTENTIAL
          PARAMETER_FILE_NAME dftd3.dat
          REFERENCE_FUNCTIONAL PBE
          TYPE DFTD3
          &PRINT_DFTD
          &END PRINT_DFTD
