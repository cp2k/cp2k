&FORCE_EVAL
  METHOD Quickstep
  &DFT
    BASIS_SET_FILE_NAME BASIS_SET
    POTENTIAL_FILE_NAME POTENTIAL
    &PRINT
      &MOMENTS
      &END
    &END
    &MGRID
      CUTOFF 200
    &END MGRID
    &QS
      EPS_DEFAULT 1.0E-8
      MAP_CONSISTENT FALSE
    &END QS
    &SCF
      EPS_SCF 1.0E-4
      SCF_GUESS ATOMIC
    &END SCF
    &XC
      &XC_FUNCTIONAL Pade
      &END XC_FUNCTIONAL
    &END XC
  &END DFT
  &SUBSYS
    &CELL
      ABC 4.0 4.0 4.0
    &END CELL
    &COORD
    O   0.000000    0.000000   -0.065587
    H   0.000000   -0.757136    0.520545
    H   0.000000    0.757136    0.520545
    &END COORD
    &KIND H
      BASIS_SET DZVP-GTH-PADE
      POTENTIAL GTH-PADE-q1
    &END KIND
    &KIND O
      BASIS_SET DZVP-GTH-PADE
      POTENTIAL GTH-PADE-q6
    &END KIND
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT H2O-VIB-MS-INT-2
  RUN_TYPE NORMAL_MODES
  PRINT_LEVEL LOW
&END GLOBAL
&VIBRATIONAL_ANALYSIS
   NPROC_REP 4
   DX 0.001
   INTENSITIES
   &PRINT
     &PROGRAM_RUN_INFO ON
     &END
   &END
   &MODE_SELECTIVE
      FREQUENCY   3300
      ATOMS 2 3
      INITIAL_GUESS ATOMIC 
      EPS_NORM  1.0E-4
      EPS_MAX_VAL 1.0E-5
   &END
&END
