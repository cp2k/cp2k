#CPQA INCLUDE ../../../data/scc/scc_parameter
#CPQA INCLUDE ../uff_table
#CPQA INCLUDE ../../../data/scc/oo.spl
#CPQA INCLUDE ../../../data/scc/oc.spl
#CPQA INCLUDE ../../../data/scc/co.spl
#CPQA INCLUDE ../../../data/scc/cc.spl
&FORCE_EVAL
  &DFT
    &QS
      METHOD DFTB
      &DFTB
        SELF_CONSISTENT    T
        ORTHOGONAL_BASIS   F   
        DO_EWALD           F
        &PARAMETER
          PARAM_FILE_PATH  ../../../data/scc
          SK_FILE   C C   cc.spl
          SK_FILE   C O   co.spl
          SK_FILE   O C   oc.spl
          SK_FILE   O O   oo.spl
        &END PARAMETER
      &END DFTB
    &END QS
    &SCF
      SCF_GUESS CORE
      &MIXING
          METHOD DIRECT_P_MIXING
          ALPHA   1.
      &END
    &END SCF
  &END DFT
  &SUBSYS
    &CELL
      ABC 20.0 20.0 20.0
      PERIODIC NONE
    &END CELL
    &COORD
     C     0.000000    0.000000    0.000000   
     O     1.100000    0.000000    0.000000  
    &END COORD
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT co
  RUN_TYPE ENERGY_FORCE
  PRINT_LEVEL HIGH
&END GLOBAL
