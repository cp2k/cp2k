# calculation should last longer than entered walltime
# program should finalize without errors

&GLOBAL
  PROJECT H2O_TMC
  PROGRAM TMC
  RUN_TYPE TMC
  PRINT_LEVEL LOW
  #TRACE
  WALLTIME 00:00:23
&END GLOBAL
&MOTION
  &TMC
      GROUP_ENERGY_SIZE 1
      GROUP_ENERGY_NR 1
      GROUP_CC_SIZE 0
      NUM_MC_ELEM 1000000
      ENERGY_FILE_NAME H2O.inp
      TEMPERATURE 330
      &MOVE_TYPE      ATOM_TRANS
        SIZE          0.5
        INIT_ACC_PROB 0.2
      &END
      NUM_MV_ELEM_IN_CELL 1
      RND_DETERMINISTIC 42
      ESIMATE_ACC_PROB .TRUE.
      INFO_OUT_STEP_SIZE 100
      RESTART_OUT 0
      PRINT_ONLY_ACC .TRUE.
      PRINT_TEST_OUTPUT
  &END TMC
&END MOTION
