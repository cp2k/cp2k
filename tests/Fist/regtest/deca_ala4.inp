
&FORCE_EVAL
  METHOD FIST
  &MM
    &FORCEFIELD
      parm_file_name ../sample_pot/deca_ala.pot
      parmtype CHM
    &END FORCEFIELD
    &POISSON
      &EWALD
        EWALD_TYPE ewald
        ALPHA .36
        GMAX 29
      &END EWALD
    &END POISSON
  &END MM
  &SUBSYS
    &CELL
      ABC 50.0 50.0 50.0
      UNIT ANGSTROM
    &END CELL
    &TOPOLOGY
      CHARGE_BETA
      COORD_FILE_NAME ../sample_pdb/deca_ala.pdb
      COORDINATE PDB
      PARA_RES
    &END TOPOLOGY
  &END SUBSYS
  STRESS_TENSOR ANALYTICAL
&END FORCE_EVAL
&GLOBAL
  PRINT_LEVEL LOW

  PROJECT deca_ala
  RUN_TYPE md
&END GLOBAL
&MOTION
  &MD
    ENSEMBLE NPT_F
    STEPS 10
    TIMESTEP 2.5
    TEMPERATURE 298
    &THERMOSTAT
       TYPE CSVR
       &CSVR
       &END
    &END
    &BAROSTAT
    &END
  &END MD
  &PRINT
     &CELL
        FILENAME =cell-deca_ala
     &END
  &END
&END MOTION
