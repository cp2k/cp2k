      ELEMENT O
    &END
    &KIND HT
      ELEMENT H
    &END
    &TOPOLOGY
       CONNECTIVITY GENERATE
       &GENERATE
        BONDPARM_FACTOR 1.2
       &END
    &END
  &END SUBSYS
&END FORCE_EVAL
&GLOBAL
  PROJECT wat_freq_freeze
  RUN_TYPE NORMAL_MODES
  #RUN_TYPE GEO_OPT
