      parm_file_name ../sample_pot/charmm.pot
      parmtype CHM
      &CHARGE
        ATOM CE2
        CHARGE -0.42
      &END CHARGE
      &CHARGE
        ATOM HE2
        CHARGE 0.21
      &END CHARGE
    &END FORCEFIELD
