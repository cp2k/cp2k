&GLOBAL
  PRINT_LEVEL LOW
  PROJECT CH2O
  RUN_TYPE DEBUG
&END GLOBAL

&DEBUG
  DEBUG_FORCES T
  DEBUG_STRESS_TENSOR F
  DX 0.0001
  EPS_NO_ERROR_CHECK 0.000001
  STOP_ON_MISMATCH F
&END DEBUG

&FORCE_EVAL
  &DFT
    &PRINT
      &AO_MATRICES
        DENSITY T
        KOHN_SHAM_MATRIX T
        &EACH
          QS_SCF 0
        &END EACH
      &END AO_MATRICES
    &END PRINT
    &QS
      METHOD xTB
      &XTB
        COULOMB_INTERACTION T
        &PARAMETER
          DISPERSION_PARAMETER_FILE dftd3.dat
        &END PARAMETER
      &END XTB
    &END QS
    &SCF
      EPS_SCF 1.e-8
      MAX_SCF 100
      SCF_GUESS MOPAC
      &MIXING
        ALPHA 0.2
        METHOD DIRECT_P_MIXING
      &END MIXING
    &END SCF
  &END DFT
  &SUBSYS
    &CELL
      ABC 20.0 20.0 20.0
      PERIODIC NONE
    &END CELL
    &COORD
      O     0.051368    0.000000    0.000000
      C     1.278612    0.000000    0.000000
      H     1.870460    0.939607    0.000000
      H     1.870460   -0.939607    0.000000
    &END COORD
  &END SUBSYS
&END FORCE_EVAL
