&GLOBAL
  PRINT_LEVEL MEDIUM
  PROGRAM_NAME TEST
  PROJECT test_pw_02
  RUN_TYPE NONE
  &TIMINGS
    THRESHOLD 0.00000000001
  &END TIMINGS
&END GLOBAL

&TEST
  @SET SIZE1 25
  @SET SIZE2 25
  @SET SIZE3 25
  &PW_TRANSFER
    GRID ${SIZE1} ${SIZE2} ${SIZE3}
    N_LOOP 10
    PW_GRID NS-FULLSPACE
    PW_GRID_BLOCKED FALSE
    PW_GRID_LAYOUT_ALL
  &END PW_TRANSFER
  &PW_TRANSFER
    GRID ${SIZE1} ${SIZE2} ${SIZE3}
    N_LOOP 10
    PW_GRID NS-FULLSPACE
    PW_GRID_BLOCKED TRUE
    PW_GRID_LAYOUT_ALL
  &END PW_TRANSFER
&END TEST
