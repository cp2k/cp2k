&GLOBAL
  PROGRAM_NAME ATOM
&END GLOBAL
&ATOM
  ELEMENT C

  RUN_TYPE PSEUDOPOTENTIAL_OPTIMIZATION

  ELECTRON_CONFIGURATION  [He] 2s2 2p2
  CORE [He]
  MAX_ANGULAR_MOMENTUM 2

  COULOMB_INTEGRALS ANALYTIC
  EXCHANGE_INTEGRALS NUMERIC

  &METHOD
     METHOD_TYPE  HARTREE-FOCK
     &XC
       &XC_FUNCTIONAL
         &XWPBE
           OMEGA 0.5
         &END XWPBE
         &PBE
           SCALE_X 0.0
           SCALE_C 1.0
         &END PBE
       &END XC_FUNCTIONAL
       &HF
         &INTERACTION_POTENTIAL
           POTENTIAL_TYPE LONGRANGE
           OMEGA 0.5
         &END INTERACTION_POTENTIAL
       &END HF
    &END XC
  &END METHOD
  &OPTIMIZATION
    EPS_SCF 1.e-5
  &END 
  &PRINT
    &BASIS_SET 
    &END 
  &END 

  &AE_BASIS
     BASIS_TYPE GEOMETRICAL_GTO
     GRID_POINTS 200
  &END AE_BASIS
  &PP_BASIS
     BASIS_TYPE GEOMETRICAL_GTO
  &END PP_BASIS
  &POTENTIAL
    PSEUDO_TYPE GTH
    &GTH_POTENTIAL
    2    2    0    0
    0.34132429077451       2   -8.80718092549534    1.52847463180286
       1
    0.30469028571068       1   10.48213397416584
    &END 
  &END POTENTIAL

  &POWELL
     ACCURACY   1.e-0
     STEP_SIZE  0.75
     WEIGHT_PSIR0 0.1
  &END

&END ATOM
